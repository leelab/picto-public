																																																																																                    //Every frame, we move the ellipse
																																																																																                    x = Ellipse.x;
																																																																																                    y = Ellipse.y;
                    

																																																																																                    if(y &gt;= 600)
																																																																																                      MovingUp.value = true;
																																																																																                    if(y &lt;= 200)
																																																																																                    MovingUp.value = false;

																																																																																                    if(MovingUp.value == true)
																																																																																                      Ellipse.setPos(x,y-10);
																																																																																                    else
																																																																																                      Ellipse.setPos(x,y+10);
                  </FrameScript>

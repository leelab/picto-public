						<Source>Stopwatch</Source>
						<SourceResult>Success</SourceResult>
						<Destination>Success</Destination>
					</Transition>
					<UIInfo><Pos x="113" y="201"/><UniqueID>{fc33aa1f-dcf7-4770-84cc-c786aaaa3172}</UniqueID></UIInfo></StateMachineElement>
				<Result><Name>Done</Name><UIInfo><Pos x="301" y="239"/><UniqueID>{82390042-fa7b-445a-a0fb-f0c4b7f6129f}</UniqueID></UIInfo></Result>
